@import "../../common/vendor.wxss";

.select-city-box {
  height: 100%;
  background-color: rgb(228, 228, 230);
}
.select-text {
  height: 80rpx;
  line-height: 80rpx;
  background-color: white;
  margin-bottom: 20rpx;
  font-size: 34rpx;
  padding-left: 40rpx;
}
.city-box {
  padding-top: 10rpx;
  background-color: white;
  padding-left: 10rpx;
}
.city-button {
  display: inline-block;
  width: 23%;
  margin-right: 2%;
  height: 70rpx;
  font-size: 32rpx;
  background-color: rgb(228, 228, 230);
}
.active {
  background-color: rgb(25, 137, 250);
}